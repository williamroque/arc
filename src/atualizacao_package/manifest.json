{
    "title": "Curva Atualização",
    "packageName": "atualizacao",
    "isDefault": false,
    "requirements": [
        "XlsxWriter==1.2.2",
        "xlrd==1.2.0",
        "numpy==1.17.3",
        "pandas==0.25.3"
    ],
    "allowedOutputExtensions": [
        {
            "name": "Excel",
            "extensions": [
                "xlsx"
            ]
        }
    ],
    "form": [
        {
            "id": "ipca",
            "label": "Adicionar Projeção",
            "type": "list",
            "inputs": [
                {
                    "label": "Período",
                    "type": "anualIncrement",
                    "incrementGroup": "period",
                    "group": "ipca-periodo",
                    "width": 50
                },
                {
                    "label": "IPCA Anual",
                    "type": "percentage",
                    "group": "ipca-anual",
                    "width": 50
                }
            ],
            "min": 1
        },
        {
            "id": "arquivo-curva",
            "label": "Arquivo Curva",
            "type": "file-input",
            "max": 1,
            "allowedExtensions": [
                {
                    "name": "Curve",
                    "extensions": [
                        "curve"
                    ]
                }
            ],
            "readToRows": {
                "id": "{}",
                "label": "Adicionar Valores — {}ª Série",
                "sync": true,
                "drawFrom": "atual",
                "inputs": [
                    {
                        "label": "Data",
                        "type": "monthlyIncrement",
                        "incrementGroup": "date",
                        "drawDefault": "starting-date",
                        "disabled": true,
                        "group": "atual-data",
                        "initialOffset": 1,
                        "width": 15
                    },
                    {
                        "label": "Juros",
                        "type": "float",
                        "group": "atual-juros",
                        "width": 21
                    },
                    {
                        "label": "Amortização",
                        "type": "float",
                        "group": "atual-amort",
                        "width": 21
                    },
                    {
                        "label": "Amex",
                        "type": "float",
                        "group": "atual-amex",
                        "width": 21
                    },
                    {
                        "label": "PU",
                        "type": "float",
                        "group": "atual-pu",
                        "width:": 21
                    },
                    {
                        "label": "Quantidade",
                        "type": "float",
                        "group": "atual-quantidade",
                        "width": 21
                    }
                ]
            }
        },
        {
            "id": "planilhas-saldo",
            "label": "Planilhas Saldo",
            "type": "file-input",
            "allowedExtensions": [
                {
                    "name": "Excel",
                    "extensions": [
                        "xlsx"
                    ]
                }
            ]
        }
    ]
}